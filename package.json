{
  "name": "supposed",
  "version": "1.0.0-beta.1",
  "description": "a test library for node.js",
  "main": "index.js",
  "scripts": {
    "build": "node build.browser.js",
    "pre-push": "npm test && node pre-push.js",
    "test": "npm run test:ts && node tests.browser && node tests",
    "test:ts": "tsc -p ./tsconfig.json && node tests.typescript/index.js",
    "watch": "nodemon -e js --exec 'node tests/index.js --tap | tap-nyan'"
  },
  "pre-push": [
    "pre-push"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/losandes/supposed.git"
  },
  "keywords": [
    "test-runner",
    "supposed",
    "javascript",
    "tap",
    "assert",
    "nodejs",
    "unit-testing",
    "node",
    "tdd",
    "bdd",
    "ddd",
    "testing",
    "test-framework",
    "ava",
    "vows",
    "tape"
  ],
  "author": "Andy Wright <andes.collab@gmail.com> (https://github.com/losandes)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/losandes/supposed/issues"
  },
  "homepage": "https://github.com/losandes/supposed#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@types/chai": "^4.2.1",
    "@types/node": "^12.7.4",
    "chai": "^4.2.0",
    "eslint": "^6.2.2",
    "eslint-config-standard": "^14.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "pre-push": "^0.1.1",
    "puppeteer": "^1.19.0",
    "tap-parser": "^9.3.3",
    "typescript": "^3.6.2"
  }
}
